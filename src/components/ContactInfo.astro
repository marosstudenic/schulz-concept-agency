---
import { getLangFromUrl, useTranslations } from "../i18n/utils";
const lang = getLangFromUrl(Astro.url);
const t = useTranslations(lang);
---

<section class="bg-white rounded-b-[4em] py-10">
  <div class="container px-4 mx-auto flex flex-col justify-center gap-2">
    <div>
      <p class="text-center lg:text-left uppercase text-black/50 w-full mb-10">
        {t("contact.founder")}
      </p>
      <div class="flex flex-col md:flex-row gap-2">
        <img
          src="/assets/images/samo-photo.png"
          alt="photo of samuel"
          class="h-40 object-contain"
        />
        <div class="my-auto px-8">
          <p class="text-xl md:text-4xl text-center md:text-left">
            Samuel Schulz
          </p>
          <p class="text-md md:text-2xl text-center md:text-left">
            samuel@schulzconcept.eu
          </p>
        </div>
      </div>

      <div class="grid grid-cols-1 md:grid-cols-2 my-32 gap-4 md:gap-28 py-20">
        <div>
          <p class="text-black/50">MAIL</p>
          <a class="text-lg md:text-3xl" href="mailto:concept@schulzconcept.eu"
            >concept@schulzconcept.eu</a
          >
        </div>
        <div>
          <p class="text-black/50 uppercase">{t("contact.phone")}</p>
          <a class="text-lg md:text-3xl" href="tel:+421911602282"
            >+421 911 602 282</a
          >
        </div>

        <div>
          <p class="text-black/50 uppercase">facebook</p>
          <a
            class="text-lg md:text-3xl"
            target="_blank"
            rel="nofollow noindex"
            href="https://www.facebook.com/profile.php?id=61552315014409"
            >Schulz Concept
          </a>
        </div>

        <div>
          <p class="text-black/50 uppercase">instagram</p>
          <a
            class="text-lg md:text-3xl"
            target="_blank"
            rel="nofollow noindex"
            href="https://www.instagram.com/schulz.concept/">schulz.concept</a
          >
        </div>

        <div>
          <p class="text-black/50 uppercase my-4">{t("contact.career")}</p>
          <div class="flex justify-center md:justify-end">
            <img src="/assets/images/kariera.png" alt="" class="h-40" />
          </div>
        </div>

        <div class="flex-col flex justify-center my-4">
          <p class="text-black text-md font-bold">
            {t("contact.career.send-cv")}
          </p>
          <p class="text-md">{t("contact.text-us")}</p>
        </div>
      </div>

